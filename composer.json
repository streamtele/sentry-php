{
    "name": "streamtele/sentry-php",
    "type": "library",
    "description": "Fork of a PHP client for Sentry (http://getsentry.com) that work also with php5.2/php5.6",
    "keywords": ["log", "logging", "sentry", "sentry-php", "php52", "php 5.2", "php56", "php6", "php 5.6"],
    "homepage": "https://github.com/streamtele/sentry-php",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "David Cramer",
            "email": "dcramer@gmail.com"
        },
        {
            "name": "Streamtele",
            "email": "developers@streamtele.com"
        }
    ],
    "require": {
        "php": ">=5.2.4",
        "ext-curl": "*"
    },
    "conflict": {
        "raven/raven": "*"
    },
    "bin": [
        "bin/sentry"
    ],
    "autoload": {
        "psr-0" : {
            "Raven_" : "lib/"
        }
    }
}
